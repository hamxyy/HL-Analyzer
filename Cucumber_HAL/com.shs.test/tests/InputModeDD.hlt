TestCase TestCase1 For BasicTuning.InputModeDD->GetPos()
	When
		hi:d8SwMacroInputModeHp = d8SwMacroInputModeHp.Microphone, 
		cap:TelecoilAvailable = TelecoilAvailable.exists, 
		cap:WlsAudio = WlsAudio.exists, 
		cap:WlsPhone = WlsPhone.exists, 
		cap:AudioshoeAvailable = AudioshoeAvailable.exists
	Then
		cap:AudioshoeAvailable = AudioshoeAvailable.exists
	Return
		1
End

TestCase TestCase2
	For 
		// This "->" can be anything even ".".
		BasicTuning.InputModeDD->GetPos()
	When
		hi:d8SwMacroInputModeHp = d8SwMacroInputModeHp.Microphone, 
		cap:TelecoilAvailable = TelecoilAvailable.exists, 
		cap:WlsAudio = WlsAudio.exists, 
		cap:WlsPhone = WlsPhone.exists, 
		cap:AudioshoeAvailable = AudioshoeAvailable.exists
	Then 
		hi:d8SwMacroInputModeHp = d8SwMacroInputModeHp.Telecoil,
		cap:WlsAudio = WlsAudio.none
	Return
		1
End