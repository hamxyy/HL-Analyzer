namespace shs_sat {	
    using shs_sat.*;
	
	package  Lib Mid_Level_Library_SpeechFocusEn{

/*$---------------------------------------------------------------------------------------------------------------*
//   Mid Level PP LIBRARY Function:    SpeechFocusEn_IsVisible
//   Input      :  <> 
//   Ouput      :  <bool> if control is visible in dependencie of pricepoints
//   Description:     
-----------------------------------------------------------------------------------------------------------------$*/
	public function bool SpeechFocusEn_IsVisible (){
        bool retval = true;            
        if (cap:PricePoint== PricePoint.PP700){
			retval = true;
        }
        else{
			retval = false;
		}
        return retval;
	}//Mid_Level_Library_SpeechFocusEn.SpeechFocusEn_IsVisible
	

